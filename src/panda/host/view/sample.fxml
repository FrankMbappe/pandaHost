<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<GridPane hgap="10" vgap="10" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="panda.host.controller.Controller">
   <padding>
      <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
   </padding>
   <columnConstraints>
      <ColumnConstraints halignment="LEFT" minWidth="100" prefWidth="100" />
      <ColumnConstraints maxWidth="Infinity" minWidth="250" prefWidth="250" />
   </columnConstraints>
   <Label fx:id="title" text="Title" textFill="#f00c0c" GridPane.columnIndex="0" GridPane.columnSpan="2" GridPane.halignment="CENTER" GridPane.rowIndex="0">
      <font>
         <Font name="Segoe UI Semibold" size="24" />
      </font>
      <GridPane.margin>
         <Insets bottom="10" left="10" right="10" top="10" />
      </GridPane.margin>
   </Label>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   
</GridPane>
